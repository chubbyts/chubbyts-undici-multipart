{
  "name": "@chubbyts/chubbyts-http-multipart",
  "type": "module",
  "version": "1.1.0",
  "description": "Chubbyts Http multipart request handling",
  "keywords": [
    "chubbyts",
    "http",
    "multipart",
    "middleware"
  ],
  "author": "Dominik Zogg",
  "license": "MIT",
  "repository": "chubbyts/chubbyts-http-multipart",
  "scripts": {
    "build": "node ./build.js",
    "cs-fix": "prettier --write src tests",
    "cs": "prettier --check src tests",
    "infection": "stryker run",
    "lint-fix": "eslint src tests --fix",
    "lint": "eslint src tests",
    "prepare": "npm run build",
    "test": "vitest"
  },
  "prettier": {
    "printWidth": 120,
    "tabWidth": 2,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "files": [
    "dist"
  ],
  "exports": {
    "./*": {
      "types": "./*.d.ts",
      "require": "./*.cjs",
      "import": "./*.mjs",
      "default": "./*.mjs"
    }
  },
  "engines": {
    "node": ">=16"
  },
  "dependencies": {
    "@chubbyts/chubbyts-http-types": "^1.2.3",
    "busboy": "^1.6.0"
  },
  "devDependencies": {
    "@chubbyts/chubbyts-eslint": "^2.0.3",
    "@chubbyts/chubbyts-function-mock": "^1.4.1",
    "@chubbyts/chubbyts-packaging": "^2.0.6",
    "@stryker-mutator/core": "^8.2.6",
    "@stryker-mutator/vitest-runner": "^8.2.6",
    "@types/busboy": "^1.5.3",
    "@types/node": "^20.11.30",
    "@types/qs": "^6.9.14",
    "@vitest/coverage-v8": "^1.6.0",
    "form-data": "^4.0.0",
    "prettier": "^3.3.2",
    "qs": "^6.12.0",
    "typescript": "^5.4.3",
    "vitest": "^1.6.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
